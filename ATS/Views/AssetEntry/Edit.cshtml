
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}





<section>

    <div class="container-fluid">
        <div class="panel panel-success" style="border-color: #64a1b9;">
            <div class="panel-heading" style="background-color: #0c717d; ">
                <h3 class="panel-title headtitle"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>Asset Edit</h3>
            </div>
            <div class="panel-body">

                <form class="form-horizontal custom-form" role="form">
                    <fieldset>
                        <div class="form-group">
                            <div class="row">
                                <div>
                                    <label class="control-label col-md-2 " for="organization" id="organizationLabel">Organization :</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlOrganization" name="Organization">
                                            <option>---Select---</option>
                                            <option>Computer</option>
                                            <option>Printer</option>
                                            <option>UPS</option>
                                        </select>
                                    </div>
                                </div>


                                <div>
                                    <label class="control-label col-md-3 " for="branch" id="branchLabel">Branch :</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlBranch" name="Branch">
                                            <option>---Select---</option>
                                            <option>Laptop</option>
                                            <option>Desktop</option>
                                            <option>Tisshue</option>
                                        </select>

                                    </div>
                                    <div class="col-md-1">
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="form-group">
                            <div class="row">
                                <div>
                                    <label class="control-label col-md-2 " for="location" id="locationLabel">Location :</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlLocation" name="Location">
                                            <option>---Select---</option>
                                            <option>Laptop</option>
                                            <option>Desktop</option>
                                            <option>Tisshue</option>
                                        </select>
                                    </div>
                                </div>


                                <div>
                                    <label class="col-md-3 control-label" for="AssetName" id="assetNameLabel">Asset Name :</label>
                                    <div class="col-md-3">
                                        <input id="assetNameTextbox" name="assetName" type="text" placeholder="Asset Name" class="form-control input-md" required />
                                    </div>
                                    <div class="col-md-1">
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="row">
                                <div>
                                    <label class="col-md-2 control-label" for="brand" id="brandLabel">Brand:</label>
                                    <div class="col-md-3">
                                        <input id="brandTextbox" name="brand" type="text" placeholder="Brand" class="form-control input-md" required />
                                    </div>
                                </div>

                                <div>
                                    <label class="col-md-3 control-label" for="assetType" id="assetTypeLabel">Asset Type:</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlAssetType" name="assetType">
                                            <option>---Select---</option>
                                            <option>Laptop</option>
                                            <option>Desktop</option>
                                            <option>Tisshue</option>
                                        </select>
                                    </div>
                                    <div class="col-md-1">
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="row">
                                <div>
                                    <label class="col-md-2 control-label" for="group" id="groupLabel">Group:</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlGroup" name="group">
                                            <option>---Select---</option>
                                            <option>Laptop</option>
                                            <option>Desktop</option>
                                            <option>Tisshue</option>
                                        </select>
                                    </div>
                                </div>

                                <div>
                                    <label class="col-md-3 control-label" for="manufacture" id="manufactureLabel">Manufacture:</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlManufacture" name="manufacture">
                                            <option>---Select---</option>
                                            <option>Laptop</option>
                                            <option>Desktop</option>
                                            <option>Tisshue</option>
                                        </select>
                                    </div>
                                    <div class="col-md-1">
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="form-group">
                            <div class="row">
                                <div>
                                    <label class="col-md-2 control-label" for="model" id="modelLabel">Model:</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlModel" name="model">
                                            <option>---Select---</option>
                                            <option>Laptop</option>
                                            <option>Desktop</option>
                                            <option>Tisshue</option>
                                        </select>
                                    </div>
                                </div>

                                <div>
                                    <label class="col-md-3 control-label" for="serialNo" id="serialNoLabel">Serial No:</label>
                                    <div class="col-md-3">
                                        <input id="serialNoTextbox" name="serialNo" type="text" placeholder="Serial No" class="form-control input-md" required />
                                    </div>
                                    <div class="col-md-1">
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">

                                <div>
                                    <label class="col-md-2 control-label" for="status" id="statusLabel">Status:</label>
                                    <div class="col-md-3">
                                        <select class="selectpicker form-control" id="ddlStatus" name="status">
                                            <option>---Select---</option>
                                            <option>Laptop</option>
                                            <option>Desktop</option>
                                            <option>Tisshue</option>
                                        </select>
                                    </div>
                                </div>

                                <div>
                                    <label class="col-md-3 control-label" for="attachment" id="attachmentLabel">Attachment:</label>
                                    <div class="col-md-2">

                                        <div class="fileinput fileinput-new" data-provides="fileinput">
                                            <span class="btn btn-default btn-file">@*<span>Choose file</span>*@<input type="file" id="attachmentFile" /></span>

                                        </div>
                                        <div class="col-md-1">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div style="text-align:right">
                            <div class="col-sm-offset-2 col-sm-10">

                                <input type="submit" id="assetEditUpdateBtn" name="assetEditUpdateBtn" value="Update" class="btn btn-group-lg btn-primary" />
                                <input type="submit" id="assetEditCancelBtn" name="assetEditCancelBtn" value="Cancel" class="btn btn-group-lg btn-warning" />
                            </div>
                        </div>

                    </fieldset>

                </form>

            </div>

        </div>


    </div>

</section>
<section>
    <div>
        <div style="text-align:left">
            
                <input type="button" id="financebtn" value="Finance" name="financebtn" class="btn btn-group-lg btn-primary" />
                <input type="button" id="servicebtn" value="service" name="servicebtn" class="btn btn-group-lg btn-primary" />
                <input type="button" id="attachmentbtn" value="Attachment" name="attachmentbtn" class="btn btn-group-lg btn-primary" />
                <input type="button" id="notesbtn" value="Notes" name="notesbtn"  class="btn btn-group-lg btn-primary"/>
                <input type="button" id="historybtn" value="History" name="historybtn" class="btn btn-group-lg btn-primary" />
               
            </div>
       
            </div>
</section>
<br/>
<section>
    <div id="servicePartialDiv">

    </div>

    <div id="partialDiv">

    </div>
    <div id="attachmentPartialDiv">

    </div>
    <div id="notesPartialDiv">

    </div>
    <div id="historypartialDiv">

    </div>
   
</section>
@section scripts{
<script src="~/Scripts/assetEdit.js"></script>
}