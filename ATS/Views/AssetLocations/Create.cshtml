
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<body>
    <div class="container-fluid">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title headtitle">@*<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>*@Asset Location Create</h3>
            </div>
            <div class="panel-body">
                <form class="form-horizontal" method="post" name="branceSetupFrom" id="branceSetupFrom">
                    <div class="form-group">
                        <label for="OrganizationName" class="col-sm-2 control-label">Organization : </label>
                        <div class="col-sm-6">
                            <select class="selectpicker form-control" id="ddlOrganizationName" name="orgName" required="">
                                <option>---Select---</option>
                                <option>Dhaka</option>
                                <option>Comilla</option>
                                <option>Chittagong</option>
                            </select>

                        </div>
                    </div>


                    <div class="form-group">
                        <label for="Branch" class="col-sm-2 control-label">Branch : </label>
                        <div class="col-sm-6">
                            <select class="selectpicker form-control" id="ddlBranch" name="orgName" required="">
                                <option>---Select---</option>
                                <option>Mogbazar</option>
                                <option>Rampura</option>
                                <option>Comilla </option>
                            </select>

                        </div>
                    </div>




                    <div class="form-group">
                        <label for="Code" class="col-sm-2 control-label">Name :</label>
                        <div class="col-sm-6">
                            <input type="text" class="form-control" id="nametextBox" name="name" placeholder="Name" required="">
                        </div>
                    </div>


                    <div class="form-group">
                        <label for="Code" class="col-sm-2 control-label">Short Name :</label>
                        <div class="col-sm-6">
                            <input type="text" class="form-control" id="assetLocShotNameTxtbox" name="shortName" readonly="readonly">
                        </div>
                    </div>

                    <div class="form-group" style="text-align:right">
                        <div class="col-sm-offset-2 col-sm-6">

                            <input type="submit" id="BranchSetupSavebtn" name="BranchSetupSavebtn" value="Save" class="btn btn-group-lg btn-success" />
                        </div>
                    </div>
                </form>
            </div>
            <div class="panel-footer" style="overflow:hidden;text-align:right;">
            </div>
        </div>


    </div>
</body>
