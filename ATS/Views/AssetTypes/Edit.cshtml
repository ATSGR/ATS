
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<body>
    <div class="container-fluid">
        <div class="panel panel-primary">
            <div class="panel-heading" style="background-color: #30686f">
                <h3 class="panel-title headtitle"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>Asset Edit</h3>
            </div>
            <div class="panel-body">
                <form class="form-horizontal" method="post" name="assetTypeForm" id="assetTypeForm">
                    <div class="form-group">
                        <label for="assetName" class="col-sm-2 control-label">Name : </label>
                        <div class="col-sm-6">
                            <input type="text" class="form-control" id="assetNametextBox" name="name" placeholder="Name" required="">

                        </div>
                    </div>


                    <div class="form-group">
                        <label for="assetSortName" class="col-sm-2 control-label">Short Name : </label>
                        <div class="col-sm-6">
                            <input type="text" class="form-control" id="assetSrtNametxtBox" name="shortName" placeholder="Short Name" required="">

                        </div>
                    </div>




                    <div class="form-group">
                        <label for="Code" class="col-sm-2 control-label">Code :</label>
                        <div class="col-sm-6">
                            <input type="text" class="form-control" id="assetCodeTextbox" readonly="readonly">
                        </div>
                    </div>



                    <div class="form-group" style="text-align:right">
                        <div class="col-sm-offset-2 col-sm-6">

                            <input type="submit" id="assetUpdate" name="assetUpdate" value="Update" class="btn btn-group-lg btn-success" />
                            <input type="submit" id="assetCancel" name="assetCancel" value="Cancel" class="btn btn-group btn-warning" />
                        </div>
                    </div>
                </form>
            </div>
            <div class="panel-footer" style="overflow:hidden;text-align:right;">
            </div>
        </div>


    </div>
</body>


