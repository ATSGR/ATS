
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="container-fluid">
    <div class="panel panel-success" style="border-color: #64a1b9;">
        <div class="panel-heading" style="background-color: #0c717d; ">
            <h3 class="panel-title headtitle">@*<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>*@Cheek In</h3>
        </div>
        <div class="panel-body">
            <h3>Asset To CheekIn</h3>
            <br />
            <div class="table-responsive">
                <table id="orgSetup" class="table table-striped table-hover table-bordered " border="0" cellpadding="0" cellspacing="0">
                    <thead style="background-color:#cde6ca">
                        <tr>
                            <th>
                                <input type="checkbox" value="">
                            </th>
                            <th>Asset No</th>
                            <th>Discription</th>
                            <th>Cheeked Out</th>
                            <th>Due</th>
                            <th>Custodium</th>
                        </tr>
                    </thead>

                    <tbody>
                        <tr>
                            <td><input type="checkbox" value=""></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>

                        </tr>

                        <tr>
                            <td><input type="checkbox" value=""></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>

                        </tr>

                        <tr>
                            <td><input type="checkbox" value=""></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>

                        </tr>


                    </tbody>
                </table>
            </div>
            <form class="form-horizontal">
                <fieldset>
                    <div class="form-group">
                        <label class="control-label col-md-2 " for="cheekInStatus" id="cheekInStatusLabel">Set Cheek In Status :</label>
                        <div class="col-md-4">
                            <select class="selectpicker form-control" id="ddlCheekInStatus" name="cheekInStatus">
                                <option>---Select---</option>
                                <option>Computer</option>
                                <option>Printer</option>
                                <option>UPS</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-md-2 " for="setLocation" id="setLocationLabel">Set Location To :</label>
                        <div class="col-md-4">
                            <select class="selectpicker form-control" id="ddlSetLocation" name="setLocation">
                                <option>---Select---</option>
                                <option>Computer</option>
                                <option>Printer</option>
                                <option>UPS</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="comments" id="commentsLabel" class="control-label col-md-2">Comments:</label>
                        <div class="col-md-4">
                            <textarea class="form-control  " rows="5" id="commentsTextarea" style="width:100%;"></textarea>
                        </div>
                    </div>

                    <div class="form-group" ">

                        <div class="checkbox inline com-md-8">
                            <label for="printCheek" id="printCheekLabel" class="control-label col-md-6">
                                <input class="col-md-0" id="printCheekbox" name="name" type="checkbox" value="">Print Cheek In Recipts
                            </label>
                        </div>




                    </div>

                    <div style="text-align:right">
                        <div class="col-sm-offset-2 col-sm-10">

                            <input type="submit" id="completeCheekInBtn" name="completeCheekInBtn" value="Complete CheekIn " class="btn btn-group-lg btn-success" />
                            <input type="button" id="assetEntryCancelBtn" name="assetEntryCancelBtn" value="Cancel" class="btn btn-group-lg btn-danger" />
                        </div>
                    </div>

                </fieldset>
            </form>
        </div>

    </div>
</div>
